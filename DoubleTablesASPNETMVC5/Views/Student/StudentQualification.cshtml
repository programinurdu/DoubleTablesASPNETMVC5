@model IEnumerable<DoubleTablesASPNETMVC5.Models.DB.StudentsQualification>

@{
    ViewBag.Title = "StudentQualification";
}

<h2>StudentQualification</h2>



<div class="row">
    <div class="col-md-2">
        @Html.Partial("_StudentMenu")
    </div>
    <div class="col-md-10">
        <div class="row">
            <div class="col-md-12">
                <p>
                    @Html.ActionLink("Create New", "Create")
                </p>
                <table class="table table-striped table-condensed table-hover">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.StudentQualificationId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.StudentId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.StartDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.EndDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.QualificationName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Result)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.StudentQualificationId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StudentId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StartDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EndDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.QualificationName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Result)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                            </td>
                        </tr>
                    }

                </table>
            </div>
        </div>
    </div>
</div>